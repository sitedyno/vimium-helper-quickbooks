{
    "manifest_version": 3,
    "name": "Vimium Helper for QuickBooks",
    "description": "Help vimium work for QuickBooks registers and javacrap forms",
    "version": "1.0.0",
    "offline_enabled": true,
    "icons": {},
    "options_ui": {},
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "all_frames": true,
            "matches": [
                "https://qbo.intuit.com/*"
            ],
            "js": [
                "script.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}
