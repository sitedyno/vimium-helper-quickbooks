{
  "manifest_version": 3,

  "name": "Vimium Helper",
  "description": "Let Vimuim see even more elements to add click hints to.",
  "version": "1.0.5",
  "offline_enabled": true,

  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"},

  "options_ui": {
    "page": "options.html"
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}